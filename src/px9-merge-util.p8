function init_menu_cartlist()
 m.sel=1
 m.options={}

 -- refresh latest list
 init_cartlist()

 for i=1,#cart_disks do
  c = cart_disks[i]
  add(m.options, c.cart)
  -- pre-select the last cart used
  if (c.cart == last_cart) then
   m.sel=i
  end
 end

 m.amt=0
 for i in all(m.options) do
  m.amt+=1
 end
 sub_mode=2
 menu_timer=0
end

function update_menu_cartlist()
 -- cancel?
 if (btnp(4)) init_menu()
 -- select?
 if btnp(5) and
 menu_timer>1 then
  -- find out which cart was selected
  for c in all(cart_disks) do
   if m.options[m.sel] == c.cart then
    -- todo: do something with this cart
    -- remember last activated cart    
    last_cart = c.cart
   end
  end
  -- jump back to main menu
  init_menu()
 end
end


--------------------------------------------------
-- export/import menu
--------------------------------------------------

function init_menu_importexport()
 m.sel=1
 m.options={}

 -- refresh latest list
 init_cartlist()

 for i=1,#cart_disks do
  c = cart_disks[i]
  add(m.options, c.cart)
  -- pre-select the last cart used
  if (c.cart == last_cart) then
   m.sel=i
  end
 end

 m.amt=0
 for i in all(m.options) do
  m.amt+=1
 end
 sub_mode=3
 menu_timer=0
end

function update_menu_importexport()
 -- cancel?
 if (btnp(4)) init_menu()
 -- select?
 if btnp(5) and
 menu_timer>1 then
  -- find out which cart was selected
  for c in all(cart_disks) do
   if m.options[m.sel] == c.cart then
    --load/save c.cart
    if last_mode == opt_export_to_target then
     -- load all sprites (+flags) into target cart (page 2)
     transfer_sprite_data(c.cart, cart_target, 0, 0)
     --transfer_flag_data(c.cart, cart_target, 0x3000, 0x3080)     
    elseif last_mode == opt_import_from_target then
     -- save all sprites (+flags) from game into data cart
     transfer_sprite_data(cart_target, c.cart, 0, 0)
     --transfer_flag_data(cart_target, c.cart, 0x3080, 0x3000)
    end
    last_cart = c.cart
   end
  end
  -- jump back to main menu
  init_menu()
 end
end

