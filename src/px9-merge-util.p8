function init_help()
 m.sel=1
 m.options={}
 m.amt=0
 sub_mode=1
end

function update_help()
 -- cancel?
 if (btnp()>0) init_menu()
end

function draw_help()
 -- display help
 local m = "\fc-purpose-\n"
 m..="\fdtHIS UTIL WAS CREATED TO\n"
 m..="MAKE IT EASIER TO MERGE\n"
 m..="SPRITESHEETS FROM MULTIPLE\n"
 m..="CARTS USING px9 COMPRESSION\n"
 m..="\n"
 m..="\fc-usage-\n"
 m..="\fd1) pUT CARTS WITH GFX IN\n"
 m..="   SAME FOLDER AS THIS ONE\n"
 m..="2) cOMPRESS TO MERGE ALL gfx\n"
 m..="   TOGETHER (IF CAN ALL FIT)\n"
 m..="3) mERGED OUTPUT WRITTEN TO\n"
 m..="   TARGET CART FILE"
 
 ?m,10,36
end

