-- main menu
--------------------------------------------------
opt_help = "info"
opt_compress = "compress -▶ "..target_name
opt_export_to_target = "export -▶ "..target_name
opt_import_from_target = "import ◀- "..target_name
opt_list = "list carts"
last_mode = ""
last_cart = ""

function init_menu()
 m={}
 m.x=8
 cx=m.x
 m.y=40
 m.options={
  opt_help,
  opt_list,
  opt_compress,
  opt_export_to_target,
  opt_import_from_target,
  "-"}
 m.amt=0
 for i in all(m.options) do
  m.amt+=1
 end
 m.sel=1
 sub_mode=0
 menu_timer=0
 -- pn-changes
 last_mode=""
 --m.visible=true
end


function update_menu()
 update_cursor()
  
 if sub_mode==0 then
  -- main menu
  if btnp(5) and
  menu_timer>1 then
   if m.options[m.sel] == opt_help then
    init_help()
     
   elseif m.options[m.sel] == opt_list then
    -- list all discovered carts (to confirm correct)
    last_mode = opt_list
    init_menu_cartlist()

  	elseif m.options[m.sel] == opt_compress then
    -- compress cart data into target cart
    -- (replacing spritesheet)
   	compress_cart_data(cart_target)
 
   elseif m.options[m.sel] == opt_export_to_target then
    -- export single cart spritesheet into target cart (replacing spritesheet)
    last_mode = opt_export_to_target
   	init_menu_importexport()

   elseif m.options[m.sel] == opt_import_from_target then
    -- import spritesheet data from target back to single cart (replacing spritesheet)
    last_mode = opt_import_from_target
   	init_menu_importexport()
   
   end

  end
 
 elseif (sub_mode==1) then
  -- display help
  update_help()
 
 elseif (sub_mode==2) then
  -- list carts menu
  update_menu_cartlist()
 
 elseif (sub_mode==3) then
  -- list carts menu
  update_menu_importexport()
 
 end

 col1=12
 col2=1
 menu_timer+=1
end

function draw_menu()
 cls(col2)
 draw_options()

 if (sub_mode==1) then
  -- display help
  draw_help()
 end
end


function init_cartlist()
 local files = ls()
 cart_disks = {}
 for name in all(files) do
  if (name ~= cart_target and sub(name,1,1)~="_") add(cart_disks, { cart=name })
 end 
end

