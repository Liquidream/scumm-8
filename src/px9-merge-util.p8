-- cartdata funcs



-----------------------------
-- decompression
-----------------------------

-- skip through compressed data
-- blocks and load the one at
-- index
function load_gfx(index,x,y) 
 local offset=base_offset
 for i=0,index-1 do
  offset += peek(offset+0) + peek(offset+1)*256 + 2
 end
 -- use sget,sset to write back
 -- to the spritesheet instead
 -- of the screen
 decomp(offset+2,x,y,pget,pset)
end


-----------------------------
-- compression
-----------------------------

-- use screen as working area
local base_offset = 0x6000


-- compress some gfx and
-- add the length of the compressed
-- data at the start (2 bytes)
function add_gfx(x,y,w,h)
 local len = px9_comp(x,y,w,h,offset+2,sget)
 poke(offset+0,len%256)
 poke(offset+1,len/256) 
 printh("wrote "..offset)
 offset += len+2
end


function compress_cart_data(target_cart)
 printh("------------------------------------------")
 printh("compress_cart_data()...")
 printh("------------------------------------------")

 cls()
 flip()

 offset = base_offset

 -- refresh latest list
 init_cartlist()
 
 for c in all(cart_disks) do
  -- import core (all data from sprite sheet > end of sprite flags)
  printh("compressing "..c.cart.."...")
  -- import all spritesheet data
  reload(0, 0, 0x2000, c.cart)
  -- compress entire spritesheet gfx (to screen working area)
  add_gfx(0,0,128,128)
 end


 --------------------------
 -- write to cart
 -- store final data in target cart sprites
 --------------------------
 -- (spritesheets)
  -- copy *everything* (leaving blank space at end)
 cstore(0x0000, 0x6000, 0x2000, target_cart)
  -- copy only what is compressed
 --cstore(0x0000, 0x6000, (offset-base_offset), target_cart)
  
 -- restore util cart spritesheet gfx data
 -- (clear out the prev cart's temp data)
 reload()
end

function transfer_sprite_data(source_cart, target_cart, source_offset, target_offset)
	-- load all sprites from source cart
 reload(0, source_offset or 0, 0x2000, source_cart)
 -- store into target cart
 cstore(target_offset or 0, 0, 0x2000, target_cart)
 -- restore util cart data
 reload()
end

function transfer_flag_data(source_cart, target_cart, source_offset, target_offset)
	-- load all sprites from source cart
 reload(0x3000, source_offset, 0x80, source_cart)
 -- store into target cart
 cstore(target_offset, 0x3000, 0x80, target_cart)
 -- restore util cart data
 reload()
end



